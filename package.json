{
  "name": "omnichanne1-react",
  "version": "1.0.0",
  "description": "An application that presents information from different APIs in one place using React on the front-end.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint client/**/*.js client/**/*.jsx webpack.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:update": "jest -u",
    "test:coverage": "jest --coverage",
    "build": "webpack -p --env.prod",
    "dev": "webpack --env.dev",
    "validate": "npm run lint",
    "watch": "webpack --env.dev --watch",
    "commit": "git-cz"
