  "author": "mcqnyc",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.15.3",
    "body-parser": "^1.17.1",
    "bootstrap": "^3.3.7",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.28.4",
    "express": "^4.14.0",
    "express-session": "^1.15.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "morgan": "^1.8.1",
    "node-ensure": "0.0.0",
    "prop-types": "^15.5.8",
    "react": "^15.4.2",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.4.2",
    "react-images": "^0.5.4",
    "react-loading": "0.0.9",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.5",
    "react-router-bootstrap": "^0.24.2",
    "react-router-dom": "^4.0.0",
    "react-sparklines": "^1.6.0",
    "react-time": "^4.3.0",
    "redux": "^3.6.0",
    "redux-promise": "^0.5.3",
    "style-loader": "^0.18.2"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.2.10",
    "babel-preset-env": "^1.5.2",
    "babel-preset-minify": "^0.2.0",
    "babel-preset-react": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "commitizen": "^2.9.5",
    "cz-conventional-changelog": "^1.2.0",
    "enzyme": "^3.3.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^2.1.1",
    "jest": "^20.0.4",
    "react-test-renderer": "^15.6.1",
    "redux-devtools-extension": "^2.13.2",
    "webpack": "^3.0.0"
  },
  "jest": {
    "testEnvironment": "jest-environment-node",
    "collectCoverageFrom": [
      "**/*.{js,jsx}", "!**/node_modules/**", "!**/vendor/**"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcqnyc/Omnichanne1-react.git"
  },
  "bugs": {
